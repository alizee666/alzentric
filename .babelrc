{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "syntax-object-rest-spread",
    "transform-class-properties"
  ],
  "env": {
    "development": {
      "plugins": [
        ["resolver", { "resolveDirs": ["src"] }, "react-hot-loader/babel"]
      ]
    },
    "production": {
      "plugins": [
        ["resolver", { "resolveDirs": ["src"] }]
      ]
    }
  }
}
